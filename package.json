{
  "name": "jsdoctext",
  "version": "0.0.1",
  "description": "A pure javascript port of catdoc",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "compile": "babel lib.js -o index.js",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konsumer/jsdoctext.git"
  },
  "keywords": [
    "word",
    "office",
    "doc"
  ],
  "author": "David Konsumer <konsumer@jetboystudio.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konsumer/jsdoctext/issues"
  },
  "homepage": "https://github.com/konsumer/jsdoctext#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "add-module-exports"
    ]
  }
}
